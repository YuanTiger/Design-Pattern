<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.my.designdemo">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".design.single.SingleActivity" />
        <activity android:name=".design.builder.BuilderActivity" />
        <activity android:name=".design.clone.CloneActivity" />
        <activity android:name=".design.factory.FactoryActivity" />
        <activity android:name=".design.absfactory.AbsFactoryActivity" />
        <activity android:name=".design.strategy.StrategyActivity" />
        <activity android:name=".design.state.StateActivity" />
        <activity android:name=".design.command.CommandActivity" />
        <activity android:name=".design.responsibility.ResponsibilityActivity" />
    </application>

</manifest>