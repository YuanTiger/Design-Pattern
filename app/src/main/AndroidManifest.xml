<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.my.designdemo">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".single.SingleActivity" />
        <activity android:name=".builder.BuilderActivity" />
        <activity android:name=".clone.CloneActivity" />
        <activity android:name=".factory.FactoryActivity" />
        <activity android:name=".absfactory.AbsFactoryActivity" />
        <activity android:name=".strategy.StrategyActivity" />
        <activity android:name=".state.StateActivity" />
    </application>

</manifest>